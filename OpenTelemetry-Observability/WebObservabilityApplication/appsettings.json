{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Microsoft.FeatureManagement": "Error"
      }
    }
  },
  "AllowedHosts": "*",
  "OpenTelemetry": {
    "CollectorUri": "http://otel-collector:4317"
  }
}
